{
  "type": "origins:multiple",
    "toggle": {
    "type": "origins:toggle",
    "active_by_default": false,
    "key": {
      "key": "key.loadToolbarActivator"
      }
    },
    "crying": {
        "type": "origins:action_over_time",
        "interval": 15,
        "entity_action": {    
          "type": "origins:execute_command",
          "command": "particle minecraft:falling_water ~ ~0.6 ~ 0.1 0.1 0.1 1 3 force",
          "permission_level": 4
      },
      "condition": {
        "type": "origins:power_active",
        "power": "allay:cosmetic/mental_breakdown_toggle"
      }
    },
    "shake": {
      "type": "origins:shaking",
      "condition": {
        "type": "origins:power_active",
        "power": "allay:cosmetic/mental_breakdown_toggle"
      }
    },
    "hidden": true
  }